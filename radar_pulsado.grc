  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [776, 1964.0]
    rotation: 0
    state: true
- name: atenuacion
  id: variable
  parameters:
    comment: ''
    value: 10**((g_ant_rx+g_ant_tx)/10)/((4*np.pi)**3*(distancia*1000)**4)
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [1704, 172.0]
    rotation: 0
    state: true
- name: butter_antialias
  id: variable
  parameters:
    comment: 'Filtro antialias

      Butterworth LP (normalizado)

      fp = 0.1

      fs = 0.2

      IL_max[dB] = 1

      Att_min[dB] = 60'
    value: scipy.signal.butter(*scipy.signal.buttord(0.1,0.2,1,60),'lowpass',output='ba')
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [2568, 476.0]
    rotation: 0
    state: true
- name: butter_fc
  id: variable
  parameters:
    comment: 'Fc

      Butterworth BP (fs = samp_rate)

      fs1 = fc-1.5e6

      fp1 = fc-0.5e6

      fp2 = fc+0.5e6

      fs2 = fc+1.5e6

      IL_max[dB] = 1

      Att_min[dB] = 60'
    value: scipy.signal.butter(*scipy.signal.buttord([fc-0.5e6,fc+0.5e6],[fc-1.5e6,fc+1.5e6],1,60,fs=samp_rate),'bandpass',output='ba',fs=samp_rate)
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [2576, 180.0]
    rotation: 0
    state: true
- name: clutter
  id: variable
  parameters:
    comment: ''
    value: '[(10/(C*k/samp_rate)**5)*np.exp(1j*C*k/samp_rate*2/(6.28*C/fc)) for k
      in range(1,1000)]'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [2192, 132.0]
    rotation: 0
    state: true
- name: codigo
